{{#if model.matured}}
<h1 class="h4">Matured Blocks</h1>
<div class="table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th>Height</th>
        <th>Block Hash</th>
        <th>Time Found</th>
        <th>Variance</th>
        <th>Reward</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.matured as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h1 class="h3">No matured blocks yet</h1>
{{/if}}
